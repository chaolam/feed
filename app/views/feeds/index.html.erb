<script type="text/x-handlebars">
{{#view App.SelectGameView}}
    <div id="overlay"></div>
    <div class="generic_dialog">
      <div class="generic_dialog_popup">
        <div id="pop_content" class="pop_content">
          <div class="close"><a href="#">X</a></div>
          <h2>Select the games you play to see the feed</h2>
          <div class="no_games_msg"><p>We cannnot detect any games on your homepage or your feed :( </p></div>
          <div class="show_games_msg">
            <p>Here are the games we think you play.</p>
            <ul id="games_list" class="game_names clearfix">
  {{#each App.gamesController.content }}
              <li class="game_name">
                <input type="checkbox" checked="checked" />
                <img src="{{unbound icon_url }}" />
                {{name}}
              </li>
  {{/each}}
            <li class="game_name"><a href="#" class="suggest">Suggest more...</a></li>
            </ul>
          </div>
          <div class="dialog_buttons"><input type="button" value="Done"/></div>
          
        </div>
      </div>
    </div>
{{/view}}
</script>

